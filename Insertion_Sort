# Insertion Sort

import sys
def insertion_sort (arr):
	for i in range (1,len(arr)):
		currNum = arr[i]

		for j in range (i-1,-1,-1):
			if arr[j] > currNum:
				arr[j+1] = arr[j]
				arr[j] = currNum
				print (arr)

			else:
				arr[j+1] = currNum
				break

test_array = [7,8,5,4,9,2]
print (insertion_sort(test_array))
